<% provide(:title, 'All users') %>
<h1>All users</h1>

<table id="users" class="display" >
<thead>
  <tr>
    <th>User</th>
    <th>Email</th>
    <th>User Since</th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to(user.username, user) %></td>
      <td><%= user.email %></td>
      <td><%= user.created_at.strftime("%B %e, %Y") %></td>
    </tr>
  <% end %>
</tbody>
</table>

